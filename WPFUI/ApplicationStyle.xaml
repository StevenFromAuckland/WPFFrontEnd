    <Style x:Uid="Style_1" x:Key="CellStyle" TargetType="{x:Type DataGridCell}">
        <Setter x:Uid="Setter_5" Property="VerticalContentAlignment" Value="Center"/>
        <!--<Setter x:Uid="Setter_6" Property="Foreground" Value="Black" />affect the white color when mouse over a row-->
        <Setter x:Uid="Setter_7" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_1" TargetType="{x:Type DataGridCell}">
                    <Border x:Uid="Border_1" Padding="2" VerticalAlignment="Center">
                        <ContentPresenter x:Uid="ContentPresenter_1" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_1" Property="IsSelected" Value="True">
                            <Setter x:Uid="Setter_6" Property="Foreground" Value="Black" />
                            <Setter x:Uid="Setter_8" Property="Background" Value="{x:Null}" />
                            <Setter x:Uid="Setter_9" Property="BorderBrush" Value="{x:Null}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Uid="Style_2" x:Key="CellStyleTextCenter" BasedOn="{StaticResource CellStyle}" TargetType="{x:Type DataGridCell}">
        <Setter x:Uid="Setter_10" Property="Block.TextAlignment" Value="Center"/>
    </Style>


