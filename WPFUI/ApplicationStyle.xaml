    <Style x:Uid="Style_9" x:Key="{x:Type TabControl}" TargetType="{x:Type TabControl}">
        <!--<Setter Property="BorderThickness" Value="3"/>
        <Setter Property="BorderBrush" Value="Red"/>
        <Setter Property="Background" Value="LightBlue"/>-->
        <Setter x:Uid="Setter_29" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_3" TargetType="TabControl">
                    <Grid x:Uid="Grid_1">
                        <Grid.RowDefinitions>
                            <RowDefinition x:Uid="RowDefinition_1" Height="Auto"/>
                            <RowDefinition x:Uid="RowDefinition_2" Height="*"/>
                        </Grid.RowDefinitions>
                        <TabPanel x:Uid="TabPanel_1" Grid.Row="0" IsItemsHost="true"/>
                        <Border  x:Uid="Border_2" Grid.Row="1" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}"  CornerRadius="5">
                            <ContentPresenter x:Uid="ContentPresenter_2" ContentSource="SelectedContent"/>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <Style x:Uid="Style_4" TargetType="TabItem">
        <Setter x:Uid="Setter_12" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_2" TargetType="TabItem">
                    <Border x:Uid="Border" Name="Border" BorderThickness="1,1,1,0" BorderBrush="Gainsboro" CornerRadius="6,6,0,0" Margin="2,0">
                        <ContentPresenter x:Uid="ContentSite" x:Name="ContentSite"
                                        TextBlock.FontWeight="Bold"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        ContentSource="Header"
                                        Margin="10,2"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_2" Property="IsSelected" Value="True">
                            <Setter x:Uid="Setter_13" TargetName="Border" Property="Background" Value="LightSkyBlue" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_3" Property="IsSelected" Value="False">
                            <Setter x:Uid="Setter_14" TargetName="Border" Property="Background" Value="GhostWhite" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

