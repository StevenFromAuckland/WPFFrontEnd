    <Style x:Uid="Style_21" TargetType="{x:Type Button}">
        <Setter x:Uid="Setter_117" Property="SnapsToDevicePixels"
          Value="true" />
        <Setter x:Uid="Setter_118" Property="OverridesDefaultStyle" Value="true" />
        <Setter x:Uid="Setter_119" Property="FocusVisualStyle"
          Value="{StaticResource ButtonFocusVisual}" />
        <Setter x:Uid="Setter_120" Property="MinHeight" Value="18" />
        <Setter x:Uid="Setter_121" Property="MinWidth" Value="25" />
        <Setter x:Uid="Setter_122" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_9" TargetType="Button">
                    <StackPanel x:Uid="StackPanel_1">
                        <!-- add StackPanel to let the button size according to the content, not according to the external available size-->
                        <Border x:Uid="Border_6" TextBlock.Foreground="{TemplateBinding Foreground}"
                x:Name="Border"
                CornerRadius="5"
                BorderThickness="1" Padding="2,1,2,1">
                            <Border.BorderBrush>
                                <LinearGradientBrush x:Uid="LinearGradientBrush_9" StartPoint="0,0"
                                 EndPoint="0,1">
                                    <LinearGradientBrush.GradientStops>
                                        <GradientStopCollection x:Uid="GradientStopCollection_6">
                                            <GradientStop x:Uid="GradientStop_22" Color="{DynamicResource BorderLightColor}"
                                Offset="0.0" />
                                            <GradientStop x:Uid="GradientStop_23" Color="{DynamicResource BorderDarkColor}"
                                Offset="1.0" />
                                        </GradientStopCollection>
                                    </LinearGradientBrush.GradientStops>
                                </LinearGradientBrush>

                            </Border.BorderBrush>
                            <Border.Background>
                                <LinearGradientBrush x:Uid="LinearGradientBrush_10" EndPoint="0.5,1"
                                 StartPoint="0.5,0">
                                    <GradientStop x:Uid="GradientStop_24" Color="{DynamicResource ControlLightColor}"
                            Offset="0" />
                                    <GradientStop x:Uid="GradientStop_25" Color="{DynamicResource ControlMediumColor}"
                            Offset="1" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Uid="CommonStates" x:Name="CommonStates">
                                    <VisualStateGroup.Transitions>
                                        <VisualTransition x:Uid="VisualTransition_1" GeneratedDuration="0:0:0.5" />
                                        <VisualTransition x:Uid="VisualTransition_2" GeneratedDuration="0"
                                  To="Pressed" />
                                    </VisualStateGroup.Transitions>
                                    <VisualState x:Uid="Normal" x:Name="Normal" />
                                    <VisualState x:Uid="MouseOver" x:Name="MouseOver">
                                        <Storyboard x:Uid="Storyboard_1">
                                            <ColorAnimationUsingKeyFrames x:Uid="ColorAnimationUsingKeyFrames_1" Storyboard.TargetProperty="(Panel.Background).
                      (GradientBrush.GradientStops)[1].(GradientStop.Color)"
                                                Storyboard.TargetName="Border">
                                                <EasingColorKeyFrame x:Uid="EasingColorKeyFrame_1" KeyTime="0"
                                         Value="{StaticResource ControlMouseOverColor}" />
                                            </ColorAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Uid="Pressed" x:Name="Pressed">
                                        <Storyboard x:Uid="Storyboard_2">
                                            <ColorAnimationUsingKeyFrames x:Uid="ColorAnimationUsingKeyFrames_2" Storyboard.TargetProperty="(Panel.Background).
                      (GradientBrush.GradientStops)[1].(GradientStop.Color)"
                                                Storyboard.TargetName="Border">
                                                <EasingColorKeyFrame x:Uid="EasingColorKeyFrame_2" KeyTime="0"
                                         Value="{StaticResource ControlPressedColor}" />
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames x:Uid="ColorAnimationUsingKeyFrames_3" Storyboard.TargetProperty="(Border.BorderBrush).
                      (GradientBrush.GradientStops)[0].(GradientStop.Color)"
                                                Storyboard.TargetName="Border">
                                                <EasingColorKeyFrame x:Uid="EasingColorKeyFrame_3" KeyTime="0"
                                         Value="{StaticResource PressedBorderDarkColor}" />
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames x:Uid="ColorAnimationUsingKeyFrames_4" Storyboard.TargetProperty="(Border.BorderBrush).
                      (GradientBrush.GradientStops)[1].(GradientStop.Color)"
                                                Storyboard.TargetName="Border">
                                                <EasingColorKeyFrame x:Uid="EasingColorKeyFrame_4" KeyTime="0"
                                         Value="{StaticResource PressedBorderLightColor}" />
                                            </ColorAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Uid="Disabled" x:Name="Disabled">
                                        <Storyboard x:Uid="Storyboard_3">
                                            <ColorAnimationUsingKeyFrames x:Uid="ColorAnimationUsingKeyFrames_5" Storyboard.TargetProperty="(Panel.Background).
                      (GradientBrush.GradientStops)[1].(GradientStop.Color)"
                                                Storyboard.TargetName="Border">
                                                <EasingColorKeyFrame x:Uid="EasingColorKeyFrame_5" KeyTime="0"
                                         Value="{StaticResource DisabledControlDarkColor}" />
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames
                      x:Uid="ColorAnimationUsingKeyFrames_6" Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)"
                                                Storyboard.TargetName="Border">
                                                <EasingColorKeyFrame x:Uid="EasingColorKeyFrame_6" KeyTime="0"
                                         Value="{StaticResource DisabledForegroundColor}" />
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames x:Uid="ColorAnimationUsingKeyFrames_7" Storyboard.TargetProperty="(Border.BorderBrush).
                      (GradientBrush.GradientStops)[1].(GradientStop.Color)"
                                                Storyboard.TargetName="Border">
                                                <EasingColorKeyFrame x:Uid="EasingColorKeyFrame_7" KeyTime="0"
                                         Value="{StaticResource DisabledBorderDarkColor}" />
                                            </ColorAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
