    <Style x:Uid="Style_11" x:Key="{x:Type RadioButton}" TargetType="{x:Type RadioButton}">
        <Setter x:Uid="Setter_31" Property="SnapsToDevicePixels" Value="true"/>
        <Setter x:Uid="Setter_32" Property="OverridesDefaultStyle" Value="true"/>
        <Setter x:Uid="Setter_33" Property="FocusVisualStyle"    Value="{StaticResource RadioButtonFocusVisual}"/>
        <Setter x:Uid="Setter_34" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_5" TargetType="{x:Type RadioButton}">
                    <BulletDecorator x:Uid="BulletDecorator_1" Background="Transparent">
                        <BulletDecorator.Bullet>
                            <Grid x:Uid="Grid_2" Width="13" 
              Height="13" >
                                <Ellipse x:Uid="Ellipse_1" x:Name="Border"  
                Fill="{StaticResource NormalBrush}"
                StrokeThickness="1"
                Stroke="{StaticResource NormalBorderBrush}" />
                                <Ellipse x:Uid="CheckMark" x:Name="CheckMark"
                Margin="4"
                Fill="{StaticResource GlyphBrush}" />
                            </Grid>
                        </BulletDecorator.Bullet>
                        <ContentPresenter 
            x:Uid="ContentPresenter_3" Margin="4,0,0,0"
            VerticalAlignment="Center"
            HorizontalAlignment="Left"
            RecognizesAccessKey="True"/>
                    </BulletDecorator>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_4" Property="IsChecked" Value="false">
                            <Setter x:Uid="Setter_35" TargetName="CheckMark" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <Trigger x:Uid="Trigger_5" Property="IsMouseOver" Value="true">
                            <Setter x:Uid="Setter_36" TargetName="Border" Property="Fill" Value="{StaticResource DarkBrush}" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_6" Property="IsPressed" Value="true">
                            <Setter x:Uid="Setter_37" TargetName="Border" Property="Fill" Value="{StaticResource PressedBrush}" />
                            <Setter x:Uid="Setter_38" TargetName="Border" Property="Stroke" Value="{StaticResource GlyphBrush}" />
                        </Trigger>
                        <Trigger x:Uid="Trigger_7" Property="IsEnabled" Value="false">
                            <Setter x:Uid="Setter_39" TargetName="Border" Property="Fill" Value="{StaticResource DisabledBackgroundBrush}" />
                            <Setter x:Uid="Setter_40" TargetName="Border" Property="Stroke" Value="#40000000" />
                            <Setter x:Uid="Setter_41" Property="Foreground" Value="#80000000"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>




